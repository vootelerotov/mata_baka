import numpy
from cvxopt import matrix
from cvxopt import solvers 
module = __import__("solver") 


def main():
    P = matrix([1.0,0.0],[0.0,1.0])
    Q = matrix([0.0,0.0])
    G = matrix ([1.0,-1.0],[1.0,1.0])
    h = (matrix((0.1,0.0)))
    
    solver = module.solver(P,Q,G,h)
    solution = solver.solve()
    print(solution)

if __name__ == '__main__':
    pass


main()